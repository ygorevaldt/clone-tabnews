{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "Minha implementação do https://www.tabnews.com.br feita para aplicar na prática os conhecimentos passados durante as aulas do https://curso.dev",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:up && next dev",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:down": "docker compose -f infra/compose.yaml down",
    "build": "next build",
    "start": "next start",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "migration:create": "node-pg-migrate -m infra/migrations create",
    "migration:up": "node-pg-migrate -m infra/migrations --envPath .env.development up",
    "migration:down": "node-pg-migrate -m infra/migrations --envPath .env.development down"
  },
  "keywords": [],
  "author": "Ygor Evaldt",
  "license": "MIT",
  "dependencies": {
    "@types/pg": "8.11.10",
    "dotenv": "16.4.4",
    "next": "13.1.6",
    "node-pg-migrate": "6.2.2",
    "pg": "8.13.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.3.0"
  },
  "devDependencies": {
    "@types/node": "22.9.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "prettier": "3.3.3",
    "typescript": "5.6.3",
    "vitest": "2.1.4"
  }
}
